{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ServiceBus" : "SERVICE_BUS_CONNECTION_STRING",
    "BlobStorage" : "BLOB_STORAGE_CONNECTION_STRING",
    "SahroDb" : "DB_CONNECTION_STRING"
  },
  "PermanentBlobStorageConfiguration" : {
    "StorageAccount" : "fileprotector",
    "AccessKey" : "PERMANENT_STORAGE_ACCOUNT_ACCESS_KEY",
    "UnFormattedUri" : "https://{0}.blob.core.windows.net"
  },
  "TemporaryBlobStorageConfiguration" : {
    "StorageAccount" : "fileprotectort",
    "AccessKey" : "STORAGE_ACCOUNT_ACCESS_KEY",
    "UnFormattedUri" : "https://{0}.blob.core.windows.net"
  }
}
